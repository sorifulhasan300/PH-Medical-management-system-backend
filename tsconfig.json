{
  "compilerOptions": {
    "module": "ESNext",
    "outDir": "./dist",
    "rootDir": "./",
    "moduleResolution": "bundler",
    "target": "ES2023",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "ignoreDeprecations": "5.0"
  },
  "include": ["src", "prisma.config.ts"],
  "exclude": ["node_modules"]
}
