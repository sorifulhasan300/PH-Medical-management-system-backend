model Payment{
      id String @id @default(uuid(7))
      amunt Float @default(0)
      transectionId String
      status PaymentStatus @default(UNPAID)
      paymentGatewayData Json
      createAt DateTime
      updatedAt DateTime
      appoitnmentId String @unique 
      appoitnment Appoitnment @relation(fields: [appoitnmentId], references: [id])

  @@map("payment_data")

}